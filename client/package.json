{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "portfolio",
  "author": "Jonathan G. Bogg",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "NODE_OPTIONS=--max-old-space-size=8192 concurrently --names \"gatsby,watch\" -c \"#663399.bgWhite,#3178c6.bgWhite\" \"gatsby develop\" \"npm:watch\"",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "watch": "concurrently \"npm:watch-*\"",
    "watch-ts": "tsc --watch",
    "watch-prettier": "onchange \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" -- prettier --write {{changed}}"
  },
  "dependencies": {
    "concurrently": "^6.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "gatsby": "^3.10.2",
    "gatsby-plugin-image": "^1.10.1",
    "gatsby-plugin-manifest": "^3.10.0",
    "gatsby-plugin-no-sourcemaps": "^3.11.0",
    "gatsby-plugin-prettier-eslint": "^1.0.6",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-remove-dependency-transpilation": "^1.0.0",
    "gatsby-plugin-sass": "^4.10.0",
    "gatsby-plugin-ts": "^3.0.0",
    "gatsby-source-sanity": "^7.0.7",
    "mini-svg-data-uri": "^1.3.3",
    "onchange": "^7.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-use": "^17.2.4",
    "sass": "^1.36.0"
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "browserslist": [
    "> 0.1%",
    "last 10 versions"
  ]
}
